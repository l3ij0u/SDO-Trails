    <validationRules>
        <fullName>PRX_CaseConclusion_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status,&quot;Closed&quot;) , 
ISBLANK(TEXT(Reason)))</errorConditionFormula>
        <errorDisplayField>Reason</errorDisplayField>
        <errorMessage>Case Conclusion is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Select_Non_first_time_right_reasons</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR($Profile.Name =&quot;System Administrator&quot;, $Profile.Name =&quot;PRX_Sales User Profile&quot;,$Profile.Name =&quot;PRX_Sales Support user&quot;), 
 ISPICKVAL(Status,&quot;Closed&quot;) ,
 ISPICKVAL(PRX_Quality_feedback__c ,&quot;Non first time right&quot;),
  ISBLANK(PRX_Non_First_time_right_reason_s__c))</errorConditionFormula>
        <errorDisplayField>PRX_Non_First_time_right_reason_s__c</errorDisplayField>
        <errorMessage>Please select Non First time right reason(s) field values before closing case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Select_Quality_feedback</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR($Profile.Name =&quot;System Administrator&quot;, $Profile.Name =&quot;PRX_Sales User Profile&quot;,$Profile.Name =&quot;PRX_Sales Support user&quot;), 
 ISPICKVAL(Status,&quot;Closed&quot;) ,ISPICKVAL(PRX_Quality_feedback__c ,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>PRX_Quality_feedback__c</errorDisplayField>
        <errorMessage>Please select quality feedback field before closing the case.</errorMessage>
    </validationRules>
</CustomObject>

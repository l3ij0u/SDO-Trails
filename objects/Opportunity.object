} else {
    window.location = &quot;https://cs83.salesforce.com/{!Opportunity.Id}/e?clone=1&amp;retURL={!Opportunity.Id}&amp;cloneli=2&amp;00N58000009RtU0=&quot;;
}</url>
    </webLinks>

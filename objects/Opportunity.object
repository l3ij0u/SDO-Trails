    <validationRules>
        <fullName>Do_Not_Accept_Special_Characters</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX( Name, &quot;[a-zA-Z0-9 ]*$&quot;))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Please do not enter any special characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Closed_Date_NotPast</fullName>
        <active>true</active>
        <description>It should not be possible to close opportunities with close date in the past . EXCPR-2581 - Release 1 - Track 1.1</description>
        <errorConditionFormula>AND(OR(ISPICKVAL( StageName , &quot;Closed Won&quot;),ISPICKVAL(StageName , &quot;Closed Lost&quot;)), CloseDate &lt; TODAY() )</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Not be possible to close an opportunity with close date in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Opp_with_noproducts_cannotbeClosed</fullName>
        <active>true</active>
        <errorConditionFormula>AND ($Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
ISPICKVAL( StageName , &quot;Closed Won&quot;),
( PRX_Opportunity_Product_Count__c  &lt; 1)
)</errorConditionFormula>
        <errorMessage>If Opportunity don&apos;t have any products,it cannot be Closed Won</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Opportunity_TotalContractValue_625k</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Amount&gt;=625000,
TEXT(StageName) ==&apos;Closed Won&apos;,Complex_Bid_Approval_Type_Child_Records__c =0,
OR( ISPICKVAL(App_status__c, &apos;&apos;),ISPICKVAL(App_status__c, &apos;Approved&apos;)))</errorConditionFormula>
        <errorMessage>Since your opportunity value has reached 625000.You will have to create approval request for complex bid before marking the opportunity as closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Reopen_Closed_Opportunity</fullName>
        <active>true</active>
        <description>It should not be possible for a any user to reopen closed opportunities. EXCPR-2580. Track 1.1 - Release 1</description>
        <errorConditionFormula>AND($Profile.Name &lt;&gt; &quot;System Administrator&quot;,
OR(ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;),ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;)) , NOT(ISBLANK( Id )))</errorConditionFormula>
        <errorMessage>An Opportunity cannot be modified if it has reached Closed Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_TotalContractValue_Is_More_Than_625k</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Amount &gt;= 625000, Complex_Bid_Approval_Type_Child_Records__c &gt;=1,Number_of_Request_Sent__c &gt; 0, TEXT(StageName) ==&apos;Closed Won&apos;, NOT(ISPICKVAL(App_status__c, &apos;Approved&apos;)))</errorConditionFormula>
        <errorMessage>Opportunity cannot be &apos;Closed Won&apos; for complex bid approval In-progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Type_Mandatory_For_SalesUser</fullName>
        <active>true</active>
        <description>Type should be mandatory when Stage is Lost. Track 1.1 - Release 1 - Not applicable to Admin</description>
        <errorConditionFormula>AND( $Profile.Name  &lt;&gt;  &quot;System Administrator&quot;, TEXT(StageName) == &quot;Closed Lost&quot; , ISBLANK(TEXT( PRX_Lost_Reason__c )) )</errorConditionFormula>
        <errorDisplayField>PRX_Lost_Reason__c</errorDisplayField>
        <errorMessage>Please enter a value for Lost Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRX_Validate_Open_Opportunities</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( App_status__c,&apos;Inprogress&apos;), ISPICKVAL( StageName , &apos;Closed Won&apos;))</errorConditionFormula>
        <errorMessage>Opportunity cannot be ¨Closed Won¨ when there is In-progress approvals</errorMessage>
    </validationRules>
